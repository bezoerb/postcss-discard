{
  "name": "postcss-discard",
  "version": "0.1.0",
  "description": "PostCSS plugin to discard rules, atrules or declarations by selector, RegExp or callback function",
  "keywords": [
    "postcss",
    "css",
    "postcss-plugin",
    "discard",
    "remove",
    "filter"
  ],
  "author": "Ben ZÃ¶rb <ben@sommerlaune.com>",
  "license": "MIT",
  "repository": "bezoerb/postcss-discard",
  "bugs": {
    "url": "https://github.com/bezoerb/postcss-discard/issues"
  },
  "engines": {
    "node": ">=6"
  },
  "homepage": "https://github.com/bezoerb/postcss-discard",
  "dependencies": {
    "common-tags": "^1.7.2",
    "lodash": "^4.17.10",
    "postcss": "^6.0.16"
  },
  "devDependencies": {
    "eslint": "^4.18.2",
    "eslint-config-postcss": "^2.0.2",
    "jest": "^22.4.2"
  },
  "scripts": {
    "test": "jest && eslint *.js"
  },
  "eslintConfig": {
    "extends": "eslint-config-postcss/es5",
    "env": {
      "jest": true
    },
    "rules": {
      "indent": [
        2,
        2,
        {
          "SwitchCase": 1
        }
      ]
    }
  },
  "prettier": {
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 80,
    "bracketSpacing": true
  },
  "jest": {
    "testEnvironment": "node"
  }
}
